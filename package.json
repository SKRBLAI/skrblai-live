{
  "name": "skrblai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "verify:env": "tsx scripts/verifyEnv.ts",
    "check:env": "node scripts/check-env.mjs",
    "env:check": "node scripts/check-env.mjs",
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "analyze": "node scripts/analyze-bundle.mjs",
    "start": "next start -H 0.0.0.0 -p 3000",
    "lint": "next lint",
    "health": "node -e \"require('http').createServer((_,r)=>r.end('ok')).listen(3000)\"",
    "clean": "rm -rf .next node_modules",
    "rebuild": "npm run clean && npm install && npm run build",
    "optimize-images": "node scripts/convert-images-webp.js",
    "image-audit": "node -e \"const fs=require('fs'),path=require('path'); fs.readdirSync('public/images').filter(f=>f.includes('agents-')).forEach(f=>{const stat=fs.statSync(path.join('public/images',f)); console.log(`${f}: ${(stat.size/1024/1024).toFixed(1)}MB`)})\"",
    "image-performance": "node scripts/image-performance-monitor.js",
    "normalize:agents": "tsx scripts/normalize-agent-images.ts",
    "validate:agents": "ts-node scripts/validateAgents.ts",
    "snap:mobile": "tsx scripts/snap-mobile.ts",
    "seed:founders": "tsx scripts/seed-founders.ts",
    "test": "echo 'No tests configured' && exit 0",
    "test:smoke": "playwright test tests/smoke.spec.ts --reporter=line",
    "test:smoke:ui": "playwright test tests/smoke.spec.ts --ui",
    "type-check": "tsc --noEmit",
    "diag:supabase": "npx tsx scripts/diagnostics/check-supabase.ts",
    "diag:auth": "echo 'Visit /ops/diag in development mode'",
    "smoke:arr": "tsx scripts/smoke-arr.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@clerk/nextjs": "^6.34.0",
    "@fontsource/inter": "^5.2.5",
    "@pinecone-database/pinecone": "^6.1.1",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@stripe/stripe-js": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/puppeteer": "^5.4.7",
    "@types/three": "^0.178.1",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.6",
    "chart.js": "^4.4.8",
    "classnames": "^2.5.1",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.6.3",
    "globby": "^15.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "next": "^15.2.4",
    "openai": "^4.100.0",
    "puppeteer": "^24.10.0",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.1",
    "react-simple-typewriter": "^5.0.1",
    "recharts": "^2.15.3",
    "resend": "^2.0.0",
    "stripe": "^14.25.0",
    "svix": "^1.81.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0",
    "twilio": "^5.7.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/next": "^8.0.7",
    "@types/node": "20.19.17",
    "@types/react": "18.3.24",
    "autoprefixer": "^10.4.21",
    "critters": "^0.0.23",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.2"
  }
}
